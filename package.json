{
  "name": "intellioverlayer",
  "displayName": "IntelliOverlayer",
  "description": "IntelliCode for Overlayer custom tags",
  "author": {
    "name": "Runas"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Runas8128/IntelliOverlayer.git"
  },
  "publisher": "Runas8128",
  "version": "0.2.1-alpha",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:python"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    },
    "virtualWorkspaces": {
      "supported": true
    }
  },
  "contributes": {
    "configuration": {
      "title": "workspace",
      "properties": {
        "intellioverlayer.workspaceFolder": {
          "type": "array",
          "default": [
            "Overlayer",
            "Scripts"
          ],
          "description": "오버레이어 커스텀 태그를 위한 워크스페이스로 인식할 폴더명입니다."
        },
        "intellioverlayer.modsFolder": {
          "type": "string",
          "default": "C:\\Program Files (x86)\\Steam\\steamapps\\common\\A Dance of Fire and Ice\\Mods",
          "description": "얼불춤 모드가 설치되어있는 폴더입니다."
        }
      }
    }
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "build": "vsce package --out build/",
    "publish": "vsce publish",
    "test": "ts-node src/test.ts"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "peggy": "^3.0.2"
  }
}
