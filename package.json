{
  "name": "intellioverlayer",
  "displayName": "Overlayer IntelliCode",
  "description": "IntelliCode for Overlayer custom tags",
  "author": {
    "name": "Runas"
  },
  "publisher": "Runas",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    },
    "virtualWorkspaces": {
      "supported": true
    }
  },
  "contributes": {
    "configuration": {
      "title": "workspace",
      "properties": {
        "intellioverlayer.workspaceFolder": {
          "type": "array",
          "default": ["Overlayer", "CustomTags", "Inits"],
          "description": "오버레이어 커스텀 태그를 위한 워크스페이스로 인식할 폴더명입니다."
        }
      }
    }
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}
